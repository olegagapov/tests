    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Agapov",
            "email": "oleg@na77.ru"
        }
    ],    "autoload": {
    "psr-4": {
        "OlegAgapov\\Tests\\": "src/"
    }
},
    "autoload-dev": {
        "psr-4": {
            "OlegAgapov\\Tests\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "OlegAgapov\\Tests\\TestsServiceProvider",
                "OlegAgapov\\Tests\\Providers\\TestsDummyServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.8.0",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "phpunit/phpcov": "~4.0|~5.0|~6.0",
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0|~8.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "orchestra/testbench": "~3.0",
        "laravel/browser-kit-testing": "~2.0.0|~3.0.0|~4.0.0|~5.0.0|~5.1",
        "orchestra/testbench-browser-kit": "~3.5|~3.6|~3.7|~3.8"
    },
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    }
}
